# Global settings
[global]
output_dir = "output"
log_level = "INFO"

# API provider configuration
[api]
openai_api_key = "YOUR_OPENAI_API_KEY_HERE"
anthropic_api_key = "YOUR_ANTHROPIC_API_KEY_HERE"

# Model configuration
[model]
openai_model = "gpt-4-turbo"
anthropic_model = "claude-3-opus-20240229"

# Source directories
[source]
# Directory containing Claude export files
claude_dir = "/path/to/Claude/export"
# Directory containing Bear notes export
bear_dir = "/path/to/Bear/export"
# Directory containing Gmail emails
gmail_dir = "/path/to/Gmail/export"
# Directory containing images to process
images_dir = "/path/to/Images"
# Directory containing XBookmarks export
xbookmarks_dir = "/path/to/XBookmarks/export"
